{
  "scripts": {
    "start": "node build/server.js",
    "dev": "tsx watch src/server.ts",
    "copy-public": "tsx ./scripts/copy-public.ts",
    "build": "tsup && yarn copy-public"
  },
  "dependencies": {
    "@hono/node-server": "^1.7.0",
    "@sentry/node": "^7.99.0",
    "hono": "^3.12.8",
    "jimp": "^0.22.10",
    "mime": "^4.0.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "eslint": "^8.56.0",
    "tsup": "^8.0.1",
    "tsx": "^3.12.2",
    "typescript": "^5.3.3"
  },
  "tsup": {
    "entry": [
      "src/server.ts"
    ],
    "outDir": "build",
    "splitting": false,
    "sourcemap": false,
    "clean": true
  },
  "license": "MIT"
}
